// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Track {
  id        String @id @default(uuid())
  stripe_id String

  message String
  status  Int    @default(0)

  @@map("tracking")
}
